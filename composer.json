{
    "name": "jonaselan/problem-two",
    "description": "Conference Track Management",
    "require": {
        "phpunit/phpunit": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "authors": [
        {
            "name": "jonaselan",
            "email": "jonas.elan@gmail.com"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "src/Tests/"
      }
    }
}
